{
  "name": "chess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mtsafe.github.io/chess/",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec \"cypress/e2e/chess/*.cy.js\"",
    "cy:allnovid": "cypress run --config video=false --spec \"cypress/e2e/chess/*.cy.js\"",
    "cy:bis": "cypress run --spec \"cypress/e2e/chess/bishop.cy.js\"",
    "cy:check": "cypress run --spec \"cypress/e2e/chess/check.cy.js\"",
    "cy:pawns": "cypress run --spec \"cypress/e2e/chess/pawns.cy.js\"",
    "cy:runai": "cypress run --spec \"cypress/e2e/chess/gameplays.cy.js\"",
    "dev": "vite --host 127.0.0.1",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "cypress": "^13.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "vite": "^7.0.0"
  }
}
